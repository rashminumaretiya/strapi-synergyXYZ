{
  "kind": "collectionType",
  "collectionName": "contact_forms",
  "info": {
    "singularName": "contact-form",
    "pluralName": "contact-forms",
    "displayName": "contactForm"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "budget": {
      "type": "enumeration",
      "enum": [
        "< RM5,000 to RM10,000",
        "RM 10,000 to RM 15,000",
        "RM 15,000 to RM 20,000",
        "> RM 20,000"
      ],
      "required": true
    },
    "typeOfService": {
      "type": "enumeration",
      "enum": [
        "Branding",
        "Creative Content",
        "Content Strategy",
        "Media Services",
        "Commerce & eCommerce",
        "Customer Relationship Management",
        "Partnership",
        "Events & Activations",
        "Data Analytics",
        "Tech"
      ],
      "required": true
    },
    "objective": {
      "type": "enumeration",
      "enum": [
        "Awareness",
        "Consideration",
        "Engagement",
        "Conversion",
        "Loyalty",
        "Advocacy"
      ]
    },
    "message": {
      "type": "text",
      "required": true
    },
    "terms": {
      "type": "boolean",
      "required": true
    }
  }
}
